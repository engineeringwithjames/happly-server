    "@typescript-eslint/parser": "4.16.1",
    "compression": "^1.7.4",
    "config": "^3.3.9",
    "dotenv": "^10.0.0",
    "eslint": "7.21.0",
    "express": "^4.18.1",
    "helmet": "^7.0.0",
    "prettier": "2.2.1",
    "socket.io": "^4.6.1",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.8.3"
